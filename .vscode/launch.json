  "version": "0.2.0",
  "configurations": [
    {
      "name": "Flutter Example",
      "request": "launch",
      "type": "dart",
      "program": "lib/main.dart", // Ruta al archivo main.dart en el proyecto example
      "cwd": "example", // Establece el directorio de trabajo en el proyecto example
      "args": [],
      "flutterMode": "debug"
    },

    {
      "type": "chrome",
      "request": "launch",
      "name": "Launch Chrome against localhost",
      "url": "http://localhost:8080",
      "webRoot": "${workspaceFolder}"
    }
  ]
}
